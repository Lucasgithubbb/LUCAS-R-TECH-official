{
  "version": 2,
  "name": "lucas-r-tech",
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/css/(.*)",
      "destination": "/css/$1"
    },
    {
      "source": "/js/(.*)",
      "destination": "/js/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}